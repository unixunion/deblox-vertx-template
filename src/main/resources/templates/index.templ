<html>
<head>

    <!--Import materialize.css-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link type="text/css" rel="stylesheet" href="/static/css/materialize.min.css"  media="screen,projection"/>

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

    <!-- The App Vars -->
    <script type="text/javascript" src="/static/app/vars.js"></script>

</head>
<body>
    <script src="/static/js/jquery.js"></script>
    <script type="text/javascript" src="/static/js/materialize.min.js"></script>
    <script type="text/javascript" src="/static/js/sockjs.min.js"></script>
    <script src="/static/js/vertx.js"></script>

    <!-- The App -->
    <script type="text/javascript" charset="utf-8" src="/static/app/app.js"></script>

    <!-- Include Nav Template -->
    @includeNamed{'nav.templ'}

    Hello @{context.get("User")} and @{context.get("bar")}
    Request path is @{context.request().path()}

    @foreach{entry: context.request().headers().entries()}
    @{entry.key}: @{entry.value}<br>
    @end{}

</body>
</html>

